<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Almot\PlatformBundle\Entity\Lokatorzy" table="lokatorzy">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="imie" type="string" column="imie" length="30" nullable="false"/>
    <field name="nazwisko" type="string" column="nazwisko" length="30" nullable="false"/>
    <field name="nazwaFirmy" type="string" column="nazwa_firmy" length="50" nullable="false"/>
    <field name="nrLokatora" type="integer" column="nr_lokatora" nullable="false"/>
    <field name="dataWyprowadzki" type="date" column="data_wyprowadzki" nullable="false"/>
    <field name="status" type="string" column="status" length="20" nullable="false"/>
    <field name="telefon" type="string" column="telefon" length="15" nullable="false"/>
    <field name="komorka" type="string" column="komorka" length="15" nullable="false"/>
    <field name="email" type="string" column="email" length="50" nullable="false"/>
    <field name="uwagi" type="string" column="uwagi" length="300" nullable="false"/>
    <field name="idZewnetrzneLokatoraDoZaliczek" type="string" column="id_zewnetrzne_lokatora_do_zaliczek" length="30" nullable="false"/>
  </entity>
</doctrine-mapping>
