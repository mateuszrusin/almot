<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Almot\AppBundle\Entity\Zarzadcy" table="zarzadcy">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idAdministracja" type="integer" column="id_administracja" nullable="true"/>
    <field name="imieNazwisko" type="string" column="imie_nazwisko" length="30" nullable="true"/>
    <field name="telefon" type="string" column="telefon" length="16" nullable="true"/>
  </entity>
</doctrine-mapping>
