<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Almot\AppBundle\Entity\Pomieszczenia" table="pomieszczenia">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idNazwaPomieszczenia" type="integer" column="id_nazwa_pomieszczenia" nullable="false"/>
    <field name="liczbaPorzadkowa" type="integer" column="liczba_porzadkowa" nullable="false"/>
    <field name="powierzchnia" type="decimal" column="powierzchnia" precision="2" scale="2" nullable="false"/>
  </entity>
</doctrine-mapping>
