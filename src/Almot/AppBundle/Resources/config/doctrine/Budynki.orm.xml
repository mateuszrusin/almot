<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Almot\AppBundle\Entity\Budynki" table="budynki">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="ulica" type="string" column="ulica" length="30" nullable="false"/>
    <field name="numer" type="string" column="numer" length="10" nullable="false"/>
    <field name="kodPocztowy" type="string" column="kod_pocztowy" length="6" nullable="false"/>
    <field name="misato" type="string" column="misato" length="20" nullable="false"/>
    <field name="powierzchniaBudynku" type="float" column="powierzchnia_budynku" precision="10" scale="0" nullable="false"/>
    <field name="uwagi" type="string" column="uwagi" length="500" nullable="false"/>
  </entity>
</doctrine-mapping>
